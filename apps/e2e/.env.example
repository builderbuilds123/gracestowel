# E2E Test Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# Storefront URL for E2E tests (default: https://localhost:5173)
STOREFRONT_URL="https://localhost:5173"

# Medusa Publishable API Key (required for storefront to connect to backend)
# Get this from Medusa Admin > Settings > API Keys
MEDUSA_PUBLISHABLE_KEY="pk_your_publishable_key_here"

# Database connection string for local development
# Used by the storefront when running locally (passed through to wrangler dev)
CLOUDFLARE_HYPERDRIVE_LOCAL_CONNECTION_STRING_HYPERDRIVE="postgresql://user:password@localhost:5432/database"
