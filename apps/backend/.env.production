# Production environment variables (Railway will inject actual values)

# PostgreSQL (internal URL, used only in Railway production)
DATABASE_URL="postgresql://<PROD_USER>:<PROD_PASSWORD>@postgres.railway.internal:5432/railway"

# Redis (internal URL)
REDIS_URL="redis://<PROD_USER>:<PROD_PASSWORD>@shortline.proxy.rlwy.net:34142"

# CORS settings for production
STORE_CORS="https://your-production-frontend.com"
ADMIN_CORS="https://your-production-admin.com"
AUTH_CORS="https://your-production-frontend.com"

# Secrets (Railway will provide secure values)
JWT_SECRET=""
COOKIE_SECRET=""
