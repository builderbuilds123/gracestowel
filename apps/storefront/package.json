{
  "name": "apps-storefront",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types && node patch-types.cjs",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "react-router dev",
    "postinstall": "node -e \"(process.env.CI || process.env.CF_PAGES === '1') ? null : require('child_process').execSync('pnpm run cf-typegen', {stdio: 'inherit'})\"",
    "preview": "vite preview",
    "local": "pnpm run build && npx wrangler dev --config dist/server/wrangler.json --port 5174",
    "lint": "echo 'Lint not configured for storefront'",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "typecheck:ci": "tsc -b",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:server": "wrangler dev -c dist/server/wrangler.json --port 5173 --ip 0.0.0.0 --inspector-port 0"
  },
  "dependencies": {
    "@medusajs/js-sdk": "^2.12.5",
    "@phosphor-icons/react": "^2.1.10",
    "@stripe/react-stripe-js": "^5.4.0",
    "@stripe/stripe-js": "^8.6.3",
    "@types/leaflet": "^1.9.21",
    "@unpic/react": "^1.0.2",
    "dompurify": "^3.3.1",
    "isbot": "^5.1.33",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.562.0",
    "posthog-js": "^1.330.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^5.0.0",
    "react-router": "^7.12.0",
    "react-router-dom": "7.12.0",
    "stripe": "^20.1.2",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.20.1",
    "@faker-js/faker": "^10.2.0",
    "@react-router/dev": "^7.12.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^25",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@vitest/coverage-v8": "^4.0.17",
    "happy-dom": "^20.3.3",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "pg": "^8.12.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.2",
    "vite": "^7.3.1",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17",
    "vitest-axe": "^1.0.0-pre.3",
    "wrangler": "^4.58.0"
  }
}
