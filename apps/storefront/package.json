{
  "name": "apps-storefront",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "react-router dev",
    "postinstall": "node -e \"(process.env.CI || process.env.CF_PAGES === '1') ? null : require('child_process').execSync('pnpm run cf-typegen', {stdio: 'inherit'})\"",
    "preview": "vite preview",
    "lint": "echo 'Lint not configured for storefront'",
    "typecheck": "node -e \"process.env.CI ? null : require('child_process').execSync('npm run cf-typegen && react-router typegen', {stdio: 'inherit'})\" && tsc -b",
    "typecheck:ci": "tsc -b",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:server": "cd dist/server && wrangler dev --port 5173 --ip 0.0.0.0"
  },
  "dependencies": {
    "@medusajs/js-sdk": "^2.12.1",
    "@phosphor-icons/react": "^2.1.10",
    "@stripe/react-stripe-js": "^5.4.0",
    "@stripe/stripe-js": "^8.5.2",
    "@types/leaflet": "^1.9.21",
    "isbot": "^5.1.31",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.554.0",
    "posthog-js": "^1.298.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "react-router": "^7.10.1",
    "stripe": "^20.0.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.15.2",
    "@faker-js/faker": "^10.1.0",
    "@react-router/dev": "^7.10.1",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@vitest/coverage-v8": "^4.0.14",
    "happy-dom": "^20.0.11",
    "jsdom": "^26.1.0",
    "msw": "^2.10.0",
    "pg": "^8.12.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.14",
    "vitest-axe": "^1.0.0-pre.3",
    "wrangler": "^4.40.2"
  }
}
